{
  "name": "topas",
  "productName": "Topas",
  "version": "0.1.0",
  "description": "Pen and Paper tool for 'the dark eye'",
  "author": "Marcel RÃ¶sler",
  "keywords": [],
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "dev": "cross-env-shell PORT=3000 BROWSER=none \"react-scripts start & wait-on http://localhost:\\$PORT && electron .\"",
    "prepack": "npm run build",
    "pack": "electron-builder -lmw",
    "eslint": "eslint",
    "electron": "electron ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.8.1",
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-brands-svg-icons": "5.8.1",
    "@fortawesome/free-regular-svg-icons": "5.8.1",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-server-express": "2.4.8",
    "apollo-upload-client": "10.0.0",
    "bootstrap-css-only": "4.3.1",
    "cors": "2.8.5",
    "electron-is-dev": "1.1.0",
    "express": "4.16.4",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.1",
    "graphql-upload": "8.0.5",
    "lokijs": "1.5.6",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-apollo": "2.5.4",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "react-select": "2.4.2",
    "react-transition-group": "2.9.0",
    "react-use": "8.1.2",
    "uuid4": "1.1.4",
    "xml-printer": "1.1.3",
    "xml-reader": "2.4.3"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "electron": "https://github.com/castlabs/electron-releases#v4.0.8-wvvmp",
    "electron-builder": "20.39.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-config-react-app": "3.0.8",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "jest-fetch-mock": "2.1.2",
    "keyboardjs": "2.5.1",
    "prettier": "1.16.4",
    "react-testing-library": "6.1.2",
    "rebound": "0.1.0",
    "typescript": "3.4.3",
    "wait-on": "3.2.0"
  },
  "build": {
    "electronDownload": {
      "mirror": "https://github.com/castlabs/electron-releases/releases/download/v",
      "cache": "./.electron-dl-cache"
    },
    "appId": "com.electron.topas",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "target": "zip"
    },
    "mac": {
      "target": "zip",
      "category": "public.app-category.games"
    },
    "win": {
      "target": "zip"
    }
  },
  "browserslist": [
    "last 2 Electron versions"
  ]
}
