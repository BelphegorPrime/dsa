{
  "name": "topas",
  "productName": "Topas",
  "version": "0.1.0",
  "description": "Pen and Paper tool for 'the dark eye'",
  "author": "Marcel RÃ¶sler",
  "keywords": [],
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "dev": "cross-env-shell PORT=3010 BROWSER=none \"react-scripts start & wait-on http://localhost:\\$PORT && electron .\"",
    "prepack": "npm run build",
    "pack": "electron-builder -lmw",
    "eslint": "eslint",
    "electron": "electron ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.9.0",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-brands-svg-icons": "5.9.0",
    "@fortawesome/free-regular-svg-icons": "5.9.0",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4-1",
    "apollo-cache-inmemory": "1.6.2",
    "apollo-client": "2.6.3",
    "apollo-server-express": "2.14.2",
    "apollo-upload-client": "10.0.1",
    "bootstrap-css-only": "4.3.1",
    "cors": "2.8.5",
    "electron-is-dev": "1.1.0",
    "express": "4.17.1",
    "graphql": "14.4.1",
    "graphql-tag": "2.10.1",
    "graphql-upload": "8.0.7",
    "htmldom": "3.0.8",
    "js-svg-path": "1.1.0",
    "lokijs": "1.5.6",
    "potrace": "2.1.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-apollo": "2.5.8",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "3.0.4",
    "react-transition-group": "4.2.0",
    "react-use": "9.7.2",
    "uuid4": "1.1.4",
    "xml-printer": "1.1.3",
    "xml-reader": "2.4.3"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "8.1.2",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.4",
    "@types/react-select": "3.0.0",
    "@types/react-transition-group": "2.9.2",
    "cross-env": "5.2.0",
    "electron": "https://github.com/castlabs/electron-releases#v4.0.8-wvvmp",
    "electron-builder": "20.44.4",
    "jest-fetch-mock": "2.1.2",
    "keyboardjs": "2.5.1",
    "prettier": "1.18.2",
    "rebound": "0.1.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react-hooks": "2.1.1",
    "typescript": "3.5.2",
    "wait-on": "3.2.0"
  },
  "build": {
    "electronDownload": {
      "mirror": "https://github.com/castlabs/electron-releases/releases/download/v",
      "cache": "./.electron-dl-cache"
    },
    "appId": "com.electron.topas",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "target": "zip",
      "category": "public.app-category.games"
    },
    "win": {
      "target": "zip"
    }
  },
  "browserslist": [
    "last 2 Electron versions"
  ]
}
