{
  "name": "topas",
  "productName": "Topas",
  "version": "0.1.0",
  "description": "Pen and Paper tool for 'the dark eye'",
  "author": "Marcel RÃ¶sler",
  "keywords": [],
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@fortawesome/fontawesome-svg-core": "1.2.7",
    "@fortawesome/free-brands-svg-icons": "5.4.2",
    "@fortawesome/free-regular-svg-icons": "5.4.2",
    "@fortawesome/free-solid-svg-icons": "5.4.2",
    "@fortawesome/react-fontawesome": "0.1.3",
    "bootstrap-css-only": "4.1.1",
    "electron-is-dev": "1.0.1",
    "eslint-plugin-react-hooks": "0.0.0",
    "prop-types": "15.6.2",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.6-next.c662dfb0 ",
    "xml-printer": "1.1.3",
    "xml-reader": "2.4.3"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "electron": "3.0.6",
    "electron-builder": "20.29.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-config-react-app": "3.0.4",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "jest-fetch-mock": "1.6.6",
    "prettier": "1.14.3",
    "react-testing-library": "5.2.3",
    "wait-on": "3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "dev": "cross-env-shell PORT=3000 BROWSER=none \"react-scripts start & wait-on http://localhost:\\$PORT && electron .\"",
    "prepack": "npm run build",
    "pack": "electron-builder -lmw"
  },
  "build": {
    "appId": "com.electron.topas",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "target": "zip"
    },
    "mac": {
      "target": "zip",
      "category": "public.app-category.games"
    },
    "win": {
      "target": "zip"
    }
  },
  "browserslist": [
    "last 1 Electron versions"
  ]
}
